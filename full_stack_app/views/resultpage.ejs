<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <title>Search Result Page</title>
</head>
<body>
    
    <%- include ('./components/headerbar.ejs') %>

    <div id="main_content_zone">
        <table>
            <tr>
                <th>Supplier Name</th>
                <th>Amount in CAN</th>
            </tr>
            <%
                var grantTable=``; 
                grants.forEach(grant => {
                    grantTable += `<tr> <td> ${grant.supplier_name} </td> <td> $${grant.amount} </td> <td></tr>`
                });
            %>
            <%- grantTable %>
        </table>
        <p><a href="/user/search">Click here to return!</a></p>
    </div>

</body>
</html>